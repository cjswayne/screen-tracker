{
  "name": "screen-tracker",
  "version": "0.0.0",
  "description": "An app to track screen time across all devices for quick easy reports.",
  "main": "index.js",
  "scripts": {
    "start": "cd ./server && npm start",
    "dev": "concurrently \"cd ./client && npm run dev\" \"cd ./server && npm run dev\"",
    "install": "concurrently \"cd ./client && npm i\" \"cd ./server && npm i\"",
    "build":"cd ./client && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cjswayne/screen-tracker.git"
  },
  "author": "cjswayne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cjswayne/screen-tracker/issues"
  },
  "homepage": "https://github.com/cjswayne/screen-tracker#readme",
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "ffi-napi": "^4.0.3",
    "mongoose": "^8.8.1",
    "robotjs": "^0.6.0",
    "systeminformation": "^5.23.5"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
